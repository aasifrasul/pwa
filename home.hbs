    <style type="text/css">
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            -webkit-tap-highlight-color: transparent
        }

        html,
        body {
            font-family: Roboto, Helvetica, Arial, sans-serif;
            font-size: 12px;
            height: 100%
        }

        #container {
            min-height: 100vh;
            background-color: #fff
        }

        a {
            text-decoration: none;
            color: #333
        }

        :not(input):not(textarea):not(button) {
            -webkit-touch-callout: none;
            -webkit-user-select: none;
            -moz-user-select: none;
            user-select: none;
            -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
        }

        #noscript {
            position: fixed;
            top: 0px;
            left: 0px;
            z-index: 3000;
            height: 100%;
            width: 100%;
            background-color: #fff
        }

        .error-page {
            background-color: #efefef;
            display: flex;
            justify-content: center;
            align-items: center;
            height: 100%;
        }

        .content {
            text-align: center;
            padding-bottom: 80px;
        }

        .image {
            vertical-align: middle;
            padding-bottom: 30px;
            padding-left: 15px;
        }

        .header {
            color: #eaeaea;
            width: 100%;
            height: 52px;
            background-color: #2874f0;
            position: absolute;
        }

        .back-button {
            padding: 13px 0px 0px 20px;
            margin: auto;
            display: inline-block;
            width: 42px;
        }

        .logo {
            margin: auto;
            padding: 13px 0px 0px 15px;
            display: inline-block;
            width: 42px;
        }

        .title {
            font-size: 16px;
            font-weight: 500;
            color: #717171;
            padding: 0px 24px 16px 24px;
        }

        .subtitle {
            letter-spacing: 1px;
            color: #909090;
            font-size: 14px;
            width: 70%;
            display: inline-block;
        }
    </style>
