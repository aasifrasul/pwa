		"compression-webpack-plugin": "1.1.11",
		"cookie-parser": "1.4.5",
		"cors": "2.8.5",
		"css-loader": "1.0.0",
		"eslint": "5.12.0",
		"eslint-loader": "2.1.1",
		"eslint-plugin-flowtype": "2.50.1",
		"eslint-plugin-import": "2.14.0",
		"eslint-plugin-jsx-a11y": "6.1.2",
