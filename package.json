{
	"name": "ally-test",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"lint:css": "stylelint 'src/**/*.css'",
		"lint:js": "eslint . --ext .js,.jsx,.ts,.tsx,.hbs",
		"prettier": "prettier --write './**/*.{js,json,jsx,ts,tsx,css}'",
		"build": "PROMO_ENV=next1 npm run build:make",
		"build-prod": "PROMO_ENV=prod npm run build:make",
		"ci-node": "npm run build:make && sh ./deployer/ci-node.sh",
		"ci-web": "sh ./deployer/ci-web.sh",
		"validate": "npm ls",
		"build:make": "node make/make.js",
		"start": "nodemon server/server.js",
		"start-dev": "NODE_ENV=development BABEL_ENV=development nodemon server.js nodemon server.js",
		"build-dev": "NODE_ENV=development BABEL_ENV=development webpack",
		"test": "nodemon scripts/test.js"
	},
	"proxy": "http://my.flipkart.com:3000",
	"browser": {
		"fs": false,
		"path": false,
		"os": false
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"lint-staged": {
		"**/*.+(js|jsx|ts|tsx)": [
			"eslint -c .eslintrc.js --ignore-path .eslintignore --fix",
			"prettier --write --ignore-path .prettierignore"
		],
		"**/*.+(json|yml|yaml|css|less|scss|md|mdx)": [
			"prettier --write --ignore-path .prettierignore"
		]
	},
	"dependencies": {
		"async": "3.2.0",
		"axios": "0.21.1",
		"fomantic-ui-css": "2.8.7",
		"idx": "2.5.6",
		"keypress.js": "2.1.5",
		"lint-staged": "11.1.2",
		"mongoose": "5.12.13",
		"prop-types": "15.7.2",
		"react": "17.0.2",
		"react-data-grid": "7.0.0-canary.46",
		"react-dom": "17.0.2",
		"react-query": "3.16.1",
		"react-redux": "7.2.4",
		"react-router-dom": "5.2.0",
		"redux": "4.1.0",
		"redux-saga": "1.1.3",
		"redux-thunk": "2.3.0",
		"regenerator-runtime": "0.13.7",
		"semantic-ui-react": "2.0.3",
		"socket.io": "4.0.2",
		"socket.io-client": "4.0.2",
		"styled-components": "5.3.0",
		"winston": "^3.8.2"
	},
